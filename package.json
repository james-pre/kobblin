{
	"name": "kobblin",
	"globalName": "Kobblin",
	"version": "0.0.1",
	"description": "A simple Typescript kernel",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [],
	"bin": {
		"build": "scripts/build.js"
	},
	"files": [
		"dist",
		"src",
		"license.md",
		"tsconfig.json",
		"eslint.shared.js"
	],
	"type": "module",
	"homepage": "https://github.com/james-pre/kobblin",
	"author": "James Prevett <jp@jamespre.dev> (https://jamespre.dev)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/james-pre/kobblin.git"
	},
	"bugs": {
		"url": "https://github.com/james-pre/kobblin/issues"
	},
	"engines": {
		"node": ">= 20"
	},
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "tsc -p tsconfig.json --noEmit && eslint src",
		"build": "tsc -p tsconfig.json && cp src/*.d.ts dist",
		"build:docs": "typedoc",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@eslint/js": "^9.16.0",
		"@types/eslint__js": "^8.42.3",
		"eslint": "^9.16.0",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"typedoc": "^0.27.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"dependencies": {
		"@types/node": "^22.10.1",
		"@zenfs/core": "^1.8.8",
		"eventemitter3": "^5.0.1",
		"ses": "^1.10.0",
		"utilium": "^1.1.1"
	}
}
